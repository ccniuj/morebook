<div class="box box-primary">
  <div class="box-header with-border">
    <h3 class="box-title">編輯分類</h3>
  </div><!-- /.box-header -->
  <div class="box-body">
    <%= form_for(@tag, {:url => dashboard_admin_tag_path, :method => 'patch'}) do |f| %>
      <div class='form-group'>
        名稱<%= f.text_field :name, :class => 'form-control' %>
      </div>
      <%= f.submit :class => 'btn btn-primary', :value => '送出' %>
    <% end %>
  </div>
</div>
